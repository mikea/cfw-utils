{
  "compilerOptions": {
    "types": ["@cloudflare/workers-types", "mocha"],
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "preserveConstEnums": true,
    "composite": true,
    "rootDir": ".",
    "module": "ESNext"
  },
  "include": ["*.ts"]
}
