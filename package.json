{
  "name": "@mikea/cfw-utils",
  "version": "0.9.0",
  "description": "cloudflare worker utils",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && npx tsc -b .",
    "clean": "rm -rf dist *.tsbuildinfo",
    "format": "prettier --write '**/*.{js,css,json,md,ts}'",
    "test": "npx mocha",
    "lint": "npx eslint *.ts",
    "watch": "rm -rf *.tsbuildinfo && npx tsc --build --watch ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikea/cfw-utils.git"
  },
  "author": "https://github.com/mikea",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/mikea/cfw-utils/issues"
  },
  "homepage": "https://github.com/mikea/cfw-utils#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "^3.4.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "mocha": "^10.0.0",
    "prettier": "^2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@mikea/ts-types": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
